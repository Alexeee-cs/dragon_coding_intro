first_digit = int(input("Give me the first digit of the value: "))
country = input("Give me the second and the third alphabetic value: ")
ID = int(input("Give me the rest of the values: "))

first=''
second=''
third=''

if first_digit == 0:
    first = 'Organic'

if first_digit == 1:
    first = 'Free range'

if first_digit == 2:
    first = 'Barn'

if first_digit == 3:
    first = 'Cage'


if country == 'UK':
    second = 'United Kindom'
if country == 'NL':
    second = 'Netherlands'
if country == 'FR':
    second = 'France'
if country == 'BE':
    second = 'Belgium'
if country == 'DE':
    second = 'Germany'
if country == 'ES':
    second = 'Spain'

third = ID

print(first,"EggCountry of Origin:",second,"Farm Id:",third)
